{
	"description": "TODO",
	"devDependencies": {
		"@testing-library/cypress": "6.0.1",
		"cypress": "5.0.0",
		"eslint": "7.7.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-cypress": "2.11.1",
		"eslint-plugin-svelte3": "2.7.3",
		"husky": "4.2.5",
		"lerna": "3.22.1",
		"lint-staged": "10.2.13",
		"prettier": "2.1.1",
		"prettier-plugin-svelte": "1.2.0",
		"start-server-and-test": "1.11.3",
		"svelte": "3.24.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"license": "TODO",
	"lint-staged": {
		"*.{svelte, html, css, scss, stylus, js, ts, json, yml, md}": [
			"prettier --write",
			"git add"
		],
		"*.{js, svelte}": [
			"eslint --fix",
			"git add"
		]
	},
	"name": "TODO",
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"endOfLine": "lf",
		"htmlWhitespaceSensitivity": "css",
		"printWidth": 80,
		"proseWrap": "preserve",
		"quoteProps": "as-needed",
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	},
	"private": true,
	"repository": "TODO",
	"scripts": {
		"bootstrap": "lerna bootstrap",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"dev:svelte": "lerna run --scope=@examples/svelte dev --stream",
		"lint": "eslint --color --ignore-path .gitignore .",
		"test": "start-server-and-test dev:svelte http://localhost:3000 cy:run"
	}
}

{
	"name": "svelte-component",
	"description": "This is atemplate for building",
	"license": "MIT",
	"repository": "https://github.com/YogliB/svelte-component-template",
	"svelte": "src/components/components.module.js",
	"module": "dist/index.min.mjs",
	"main": "dist/index.min.js",
	"devDependencies": {
		"@babel/core": "7.5.5",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-transform-runtime": "7.5.5",
		"@babel/preset-env": "7.5.5",
		"@testing-library/jest-dom": "4.0.0",
		"@testing-library/svelte": "1.7.1",
		"@types/jest": "24.0.16",
		"autoprefixer": "9.6.1",
		"babel-jest": "24.8.0",
		"eslint": "6.1.0",
		"eslint-config-prettier": "6.0.0",
		"eslint-plugin-jest": "22.14.1",
		"eslint-plugin-svelte3": "2.7.3",
		"husky": "3.0.2",
		"jest": "24.8.0",
		"jest-transform-svelte": "2.0.2",
		"lint-staged": "9.2.1",
		"npm-run-all": "4.1.5",
		"postcss": "7.0.17",
		"prettier": "1.18.2",
		"rollup": "1.18.0",
		"rollup-plugin-babel": "4.3.3",
		"rollup-plugin-commonjs": "10.0.2",
		"rollup-plugin-livereload": "1.0.1",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-svelte": "5.1.0",
		"rollup-plugin-terser": "5.1.1",
		"sirv-cli": "0.4.4",
		"svelte": "3.7.0",
		"svelte-preprocess": "2.15.0"
	},
	"scripts": {
		"bootstrap": "lerna bootstrap",
		"lint": "eslint --color ./**/*.js ./**/*.svelte",
		"test": "jest --verbose",
		"build": "lerna build"
	},
	"browserslist": [
		"defaults"
	],
	"keywords": [
		"svelte"
	],
	"files": [
		"src/components",
		"dist/index.min.mjs",
		"dist/index.min.js"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js, svelte}": [
			"eslint --fix",
			"git add"
		],
		"*.{html, css, scss, stylus, js, ts, json, yml, md}": [
			"prettier --write",
			"git add"
		]
	}
}
